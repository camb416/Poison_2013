<settings>
<!-- how long to wait after interacting before the hands come up -->
<touchtimeout>10.0</touchtimeout>

<!-- how long does each hand stay there before crossfading to the next one -->
<timebetweenprompts>3.0</timebetweenprompts>

<!-- just keep this to three (the max hands you'll see -->
<numtouchprompts>3</numtouchprompts>

</settings>